{
  "name": "monprojet/gestion-boutique",
  "description": "Description de votre projet",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.2",
    "ext-pdo": "*",
    "illuminate/database": "^11.13",
    "slim/psr7": "^1.7",
    "slim/slim": "4.*",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
      "psr-4": {
          "App\\": "src/"
      }
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "friendsofphp/php-cs-fixer": "^3.0"
  },
  "scripts": {
      "test": "phpunit",
      "fix": "php-cs-fixer fix"
  },
  "config": {
      "optimize-autoloader": true,
      "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
